            <property name="x">16</property>
            <property name="y">201</property>
          </packing>
        </child>
        <child>
          <widget class="GtkLabel" id="label_show">
            <property name="width_request">272</property>
            <property name="height_request">80</property>
            <property name="visible">True</property>
          </widget>
          <packing>
            <property name="x">23</property>
            <property name="y">17</property>
          </packing>
        </child>
      </widget>
    </child>
  </widget>
</glade-interface>
