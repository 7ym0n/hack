          </packing>
        </child>
        <child>
          <widget class="GtkLabel" id="label_help_button">
            <property name="width_request">296</property>
            <property name="height_request">50</property>
            <property name="visible">True</property>
            <property name="label" translatable="yes">&#x79FB;&#x52A8;&#x9F20;&#x6807;&#x5230;&#x6309;&#x94AE;&#x4E0A;&#x53EF;&#x770B;&#x5230;&#x7B80;&#x5355;&#x5E2E;&#x52A9;</property>
